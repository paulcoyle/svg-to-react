{
  "name": "@paulcoyle/svg-to-react",
  "description": "An opinionated utility for converting SVG files to React components.",
  "keywords": [
    "svg",
    "react",
    "utility",
    "converter"
  ],
  "version": "1.0.0",
  "cli": "dist/cli.js",
  "bin": {
    "svg-to-react": "dist/cli.js"
  },
  "engines": {
    "node": ">= 14"
  },
  "targets": {
    "cli": {
      "context": "node",
      "source": "src/index.ts",
      "sourceMap": false
    }
  },
  "scripts": {
    "build": "env-cmd parcel build",
    "pack": "env-cmd packito --nopublish",
    "publish": "env-cmd packito",
    "watch": "env-cmd parcel watch",
    "lint": "env-cmd eslint --ext .ts .",
    "lint-fix": "npm run lint -- --fix",
    "format": "env-cmd --silent prettier -l **/*.ts",
    "format-fix": "npm run format -- --write"
  },
  "author": {
    "name": "Paul Coyle",
    "email": "paul@paulcoyle.com",
    "url": "https://www.paulcoyle.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/paulcoyle/svg-to-react"
  },
  "bugs": {
    "url": "https://github.com/paulcoyle/svg-to-react/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "@parcel/packager-ts": "^2.0.1",
    "@parcel/transformer-typescript-types": "^2.0.1",
    "@types/ejs": "^3.1.0",
    "@types/fs-extra": "^9.0.13",
    "@types/glob": "^7.2.0",
    "@types/jest": "^27.0.3",
    "@types/svgo": "^2.6.0",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-testing-library": "^5.0.1",
    "jest": "^27.4.5",
    "packito": "^0.5.0",
    "parcel": "^2.0.1",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "commander": "^8.3.0",
    "ejs": "^3.1.6",
    "glob": "^7.2.0",
    "picocolors": "^1.0.0",
    "svgo": "^2.8.0"
  }
}
